Request = {
    metodo: str
    percorso: str
    body: str
}

Response = {
    status: int
    body: str
    tipo: str
}

@native("flow_http_response")
fn response(status: int, body: str, tipo: str): Response

@native("flow_http_serve")
fn serve(porta: int, handler: fn(Request): Response)

